@model IEnumerable<ck.Models.Genre>

<ul class="list-unstyled">
    @foreach (var genre in Model)
    {
        <li>
            <a class="dropdown-item" href="@Url.Action("Genre", "Movies", new { id = genre.Id })">
                <i class="fa fa-chevron-right font_12 me-1"></i> @genre.Name
            </a>
        </li>
    }
</ul>
